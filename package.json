{
  "name": "simple-node-server",
  "version": "1.0.0",
  "description": "A simple enough node server scaffolding to get you started on your project immediately",
  "main": "index.js",
  "engines": {
    "node": ">=10.13.0"
  },
  "_moduleAliases": {
    "@root": "server",
    "@core": "server/core",
    "@api": "server/api",
    "@config": "config/"
  },
  "scripts": {
    "start": "DEBUG=simpleNodeServer node index.js",
    "dev": "nodemon -r dotenv",
    "test": "",
    "lint": "eslint server/",
    "add-resource": "node ./tools/cli.js"
  },
  "keywords": [
    "node",
    "scaffold",
    "expressjs",
    "server",
    "node",
    "project",
    "nodejs",
    "server",
    "simple",
    "http",
    "boilerplate",
    "api-design",
    "microservices",
    "backend"
  ],
  "author": "Munim Dibosh",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/firestore": "^4.9.7",
    "@google-cloud/kms": "^2.3.1",
    "@google-cloud/storage": "^5.8.2",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "debug": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "handlebars": "^4.7.7",
    "http-status-codes": "^1.3.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.7",
    "sax": "^1.2.4",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "eslint": "5.10.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0"
  }
}
